<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>NewMatchServlet</servlet-name>
        <servlet-class>servlet.NewMatchServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NewMatchServlet</servlet-name>
        <url-pattern>/new-match</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MatchScoreServlet</servlet-name>
        <servlet-class>servlet.MatchScoreServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MatchScoreServlet</servlet-name>
        <url-pattern>/match-score</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MatchesServlet</servlet-name>
        <servlet-class>servlet.MatchesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MatchesServlet</servlet-name>
        <url-pattern>/matches</url-pattern>
    </servlet-mapping>


</web-app>